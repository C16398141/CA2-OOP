class Droids extends Snakehead {
 
  float cx;
  float cy;
  float r;
  
  Droids(Snakehead head){
    cx=head.x;
    cy=head.y;
    diameter=5;
    r=head.diameter*2;
  }
  
  void draw(Snakehead head)
  {
    cx=head.x;
    cy=head.y;
    float t = millis()/400.0f;
    int x = (int)(cx+r*cos(t));
    int y = (int)(cy+r*sin(t));
    
    ellipse(x,y,diameter,diameter);

  }
}
